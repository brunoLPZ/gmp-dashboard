{
  "production": false,
  "settings": {
    "gmpServiceUrl": "http://localhost:8070/gmp-service",
    "keycloak": {
      "config": {
        "url": "http://keycloak:8080/auth",
        "realm": "gmp",
        "clientId": "gmp-ui"
      },
      "initOptions": {
        "onLoad": "login-required",
        "checkLoginIframe": false
      },
      "enableBearerInterceptor": true,
      "bearerExcludedUrls": ["/assets"]
    }
  }
}
